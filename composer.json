{
    "name": "laravel-notification-channels/clicksend",
    "description": "Clicksend notification for laravel",
    "homepage": "https://github.com/laravel-notification-channels/clicksend",
    "license": "MIT",
    "authors": [
        {
            "name": "Iliyas M",
            "email": "im.smiliyas@gmail.com",
            "homepage": "https://smiliyas.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.2",
        "clicksend/clicksend-php": "^5.0",
        "illuminate/notifications": "~6.0 || ~7.0 || ~8.0",
        "illuminate/support": "~6.0 || ~7.0 || ~8.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "NotificationChannels\\Clicksend\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NotificationChannels\\Clicksend\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "test:coverage": "phpunit --coverage-text --coverage-clover=coverage.clover"
    },
    "config": {
        "sort-packages": true
    }
}
